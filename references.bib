	keywords = {*Gene Library, *Gene Ontology, *User-Computer Interface, Benchmarking, Computational Biology/*methods/statistics \& numerical data, Databases, Genetic, Gene Expression Profiling, Genome, Human, Humans, Internet, Molecular Sequence Annotation},
	pages = {W90--97},

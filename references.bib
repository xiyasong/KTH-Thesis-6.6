	keywords = {*Genomics, Amino Acid Sequence, DNA Mutational Analysis, Genes, Neoplasm/*genetics, Genome, Human/*genetics, Humans, Molecular Sequence Data, Mutation/*genetics, Neoplasm Proteins/chemistry/genetics, Neoplasms/*genetics, Protein Kinases/chemistry/genetics},
